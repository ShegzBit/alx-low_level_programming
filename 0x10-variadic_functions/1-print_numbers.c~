#include <stdarg.h>
#include <stdio.h>
#include "variadic_functions.h"

/**
 * print_numbers - prints the numbers passed to it
 * @separator : string to print between the numbers
 * @n : number of numbers
 * Return: void
 */

void print_numbers(const char *separator, const unsigned int n, ...)
{
  unsigned int i;
  va_list ap;
  va_start(ap, n);
  for (i = 0; i < n; i++)
    {
      printf("%d", va_arg(ap, int));
      printf("%s", separator);
    }
  va_end(ap);
  putchar('\n');
}
