#include <stdio.h>
/**
 * _islower - Short description, single line
(* a blank line
* c : for char test
* Description: checks if lowercase)?
(* section header: Section description)*
* Return: Description of the returned value
*/
int _islower(int c)
{
if (c >= 97 && c <= 122)
{
return (1);
}
else
{
return (0);
}
}
